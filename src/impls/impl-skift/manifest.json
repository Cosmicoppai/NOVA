{
    "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.component.v1",
    "id": "impl-skift",
    "type": "lib",
    "description": "Karm platform embedding layer for skift.",
    "enableIf": {
        "sys": [
            "skift"
        ]
    },
    "requires": [
        "crts",
        "abi",
        "hjert-api",
        "libheap"
    ],
    "provides": [
        "embed-sys-impl",
        "embed-ui-impl",
        "embed-base-impl",
        "embed-logger-impl"
    ]
}